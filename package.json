{
  "name": "cat-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/cat-components/cat-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "func() { hygen component \"$1\" --domain=\"$2\" \"$3\" && stencil build; }; func"
  },
  "dependencies": {
    "@bit/bit.envs.compilers.stencil": "file:./components/compilers/stencil",
    "@stencil/core": "^2.0.1"
  },
  "license": "MIT",
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/stencil@0.0.35"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "devDependencies": {
    "@stencil/sass": "^1.1.1"
  }
}
